<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5"/>
    <!--timeout sessions kendini otomatik sonlandırması zaman aşımını dakika cinsinden ayarlar aynı 
     zamanda cookiless "cookisiz" demektir bu durumu true ile aktif hale getirerek sitenin karşı bilgisayardan 
     sadece sessions nesneleri ile almasını sağlanabiliriz.
     -->
    <sessionState timeout="1" cookieless="true"/>
  </system.web>
</configuration>